{
  "input": [
    "src/_includes"
  ],
  "output": "src/static/css",
  "variables": {
    "css": true,
    "sass": false
  },
  "colors": [
    {
      "key": "white",
      "color": "rgba(255, 255, 255, 1)",
      "scale": {
        "light": "auto",
        "lighter": "auto",
        "lightest": "auto",
        "dark": "auto",
        "darker": "auto",
        "darkest": "auto"
      }
    },
    {
      "key": "black",
      "color": "#212130",
      "scale": {
        "light": "auto",
        "lighter": "auto",
        "lightest": "auto",
        "dark": "auto",
        "darker": "auto",
        "darkest": "auto"
      }
    },
    {
      "key": "primary",
      "color": "#9D5CFF",
      "scale": {
        "light": "auto",
        "lighter": "auto",
        "lightest": "auto",
        "dark": "auto",
        "darker": "auto",
        "darkest": "auto"
      }
    },
    {
      "key": "accent",
      "color": "#53FFE0",
      "scale": {
        "light": "auto",
        "lighter": "auto",
        "lightest": "auto",
        "dark": "auto",
        "darker": "auto",
        "darkest": "auto"
      }
    },
    {
      "key": "font",
      "color": "#53FFE0",
      "scale": {
        "light": "#ccccdb",
        "lighter": "auto",
        "lightest": "auto",
        "dark": "#2a2b38",
        "darker": "auto",
        "darkest": "auto"
      }
    }
  ],
  "containers": [
    {
      "key": "s",
      "maxWidth": "39rem"
    },
    {
      "key": "m",
      "maxWidth": "60rem"
    },
    {
      "key": "l",
      "maxWidth": "80rem"
    }
  ],
  "fonts": [
    {
      "key": "sans",
      "family": "'Inter', sans-serif"
    },
    {
      "key": "serif",
      "family": "serif"
    },
    {
      "key": "script",
      "family": "script"
    },
    {
      "key": "mono",
      "family": "monospace"
    }
  ],
  "baseFontSizes": [
    {
      "size": "100%",
      "query": "screen"
    },
    {
      "size": "112.5%",
      "query": "screen and (min-width: 64em)"
    },
    {
      "size": "118%",
      "query": "screen and (min-width: 100em)"
    }
  ],
  "baseLineHeight": "1.3",
  "typeScale": "1.33",
  "gradients": [
    {
      "key": "divider",
      "gradient": "linear-gradient(90deg, #53FFE0, #9D5CFF)",
      "fallback": "#53FFE0"
    },
    {
      "key": "primaryRadial",
      "gradient": "radial-gradient(rgba(157,92,255, 1), rgba(42, 44, 61, 1))",
      "fallback": "rgba(157,92,255, 1)"
    },
    {
      "key": "accentRadial",
      "gradient": "radial-gradient(rgba(83, 255, 224, 1), rgba(42, 44, 61, 1))",
      "fallback": "rgba(83, 255, 224, 1)"
    }
  ],
  "media": [
    {
      "key": "b",
      "query": null
    },
    {
      "key": "p",
      "query": "print"
    },
    {
      "key": "xs",
      "query": "screen and (min-width: 25em)"
    },
    {
      "key": "s",
      "query": "screen and (min-width: 50em)"
    },
    {
      "key": "m",
      "query": "screen and (min-width: 64em)"
    },
    {
      "key": "l",
      "query": "screen and (min-width: 100em)"
    }
  ],
  "radius": [
    {
      "key": "rounded",
      "radius": "5px"
    },
    {
      "key": "pill",
      "radius": "50rem"
    },
    {
      "key": "circle",
      "radius": "100%"
    }
  ],
  "shadows": [
    {
      "key": "xs",
      "shadow": "0 0.1rem 0.2rem 0 rgba(0, 0, 0, .2)"
    },
    {
      "key": "s",
      "shadow": "0 0.25rem 0.5rem -0.05rem rgba(0, 0, 0, .2)"
    },
    {
      "key": "m",
      "shadow": "0 0.4rem 0.7rem -0.1rem rgba(0, 0, 0, .2)"
    },
    {
      "key": "l",
      "shadow": "0 0.55rem 1rem -0.2rem rgba(0, 0, 0, .2)"
    },
    {
      "key": "xl",
      "shadow": "0 0.7rem 1.5rem -0.3rem rgba(0, 0, 0, .2)"
    }
  ],
  "transitions": {
    "durations": [
      {
        "key": "default",
        "duration": ".2s"
      }
    ],
    "functions": [
      {
        "key": "ease",
        "function": "ease"
      }
    ],
    "delays": [
      {
        "key": "none",
        "delay": "0s"
      }
    ]
  }
}