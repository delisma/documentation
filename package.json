{
  "name": "hydrogen-documentation",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.2.2",
    "@hydrogen-design-system/hydrogen.css": "^2.0.0-beta.4",
    "alpinejs": "^3.10.2",
    "browser-sync": "^2.27.10",
    "cross-env": "^7.0.2",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.14.0",
    "luxon": "^1.25.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "prismjs": "^1.28.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel hydrogenWatch eleventyServe",
    "eleventyServe": "eleventy --serve --quiet",
    "eleventyBuild": "eleventy",
    "hydrogenWatch": "npx h2-watch",
    "hydrogenBuild": "npx h2-build",
    "debug": "set DEBUG=* & eleventy",
    "build": "npm-run-all --sequential hydrogenBuild eleventyBuild",
    "browsersync": "browser-sync start --server '_site' --startPath 'en/index.html' --files '_site' --port 8080 --no-notify --no-open"
  },
